{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules":{
    "react/prefer-stateless-function": 0,
    "no-use-before-define": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types":0,
    "react/jsx-no-bind":0,
    "key-spacing":0,
    "class-methods-use-this":0
  },
  "parser": "babel-eslint"
}
